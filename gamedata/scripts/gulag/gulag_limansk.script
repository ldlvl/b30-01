function checkStalker(npc_community, gulag_type, npc_rank, se_obj)
	--Lim Lager Bandits--
	if gulag_type=="lim_lager_bandits" then
		return npc_community=="bandit"
	end

	--Lim Lager Freedom Stalkers--
	if gulag_type=="lim_lager_freedom_stalkers" then
		return npc_community=="freedom" or npc_community=="stalker"
	end

	return false
end

