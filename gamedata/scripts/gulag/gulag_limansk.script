function load_states(gname, type)
	--Lim Lager Bandits--
	if type=="lim_lager_bandits" then
		return function(gulag)
			if db.actor~=nil then
				if (level.get_time_hours()*60+level.get_time_minutes()) >= 380 and (level.get_time_hours()*60+level.get_time_minutes()) < 1345 then
					return 0  -- день
				else
					return 1  -- ночь
				end
			else
				return gulag.state
			end
		end
	end

	--Lim Lager Freedom Stalkers--
	if type=="lim_lager_freedom_stalkers" then
		return function(gulag)
			if db.actor~=nil then
				if (level.get_time_hours()*60+level.get_time_minutes()) >= 380 and (level.get_time_hours()*60+level.get_time_minutes()) < 1345 then
					return 0  -- день
				else
					return 1  -- ночь
				end
			else
				return gulag.state
			end
		end
	end
	
end

