function is_scaf()
    local armor = actor_active_item( 6 )
    if armor and
    ( strpos( armor:section(), "scientific", 1, true )
    or strpos( armor:section(), "ecolog", 1, true ) )
    then return true
    else return false
    end
end

function is_not_scaf() return not is_scaf() end

