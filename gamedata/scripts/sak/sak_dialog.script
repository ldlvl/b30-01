        local obj = first_speaker:object( "af_compass_dyn" .. n .. "d" )
        if obj then
            first_speaker:transfer_item( obj, second_speaker )
            sak.relocate_item( second_speaker, "in", "af_compass", 1 )
        end
    end
end

