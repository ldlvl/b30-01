function weapon_unload(arg)
  if arg then
    local wpn = false
    if type(arg) == 'userdata' then
       wpn = arg
    else
       wpn = client_obj(tonumber(arg))
    end

    if wpn then
       wpn:unload_magazine()
    else
       wprintf("[~T].~C0C #ERROR: weapon_unload failed for weapon id [%s]~C07", DumpVar(arg))
    end -- if wpn
  end -- if arg
end

